[$ sub left_nav $]
[-	
	$include = shift;
	$req = shift;
-]
          <td width="201" valign="top" bgcolor="#E7EFF7"><img src="/graphics/nav-lefttopdots.gif" alt=" " width="201" height="24">
            <table width="100%" border="0" cellspacing="0" cellpadding="0">
              <tr> 
                <td width="100%" valign="top" class="xsm">
[- Execute($req->{include_dir}."/manage/".$include,$req); -]
</td>
	</tr></table>

	<td width="1" valign="top" background="/graphics/bg-dotsvert.gif"><img src="/graphics/spacer.gif" alt=" " width="1" height="1" border="0"></td>
[$ endsub $]

[$ sub chooser_link $]
        [- $type = shift;
        $pform = shift;
        $field = shift;
        $text = shift;
	$req=shift;
        $fname = sprintf('open_%s_chooser', $type);
        -]
<script language="JavaScript">
<!--
function [+$fname+] (){
        param = "directories=no,menubar=no,toolbar=yes,scrollbars=yes,resizable=yes,width=" + 650 + ",height=" + 450;
        current_selection = document.[+$pform+].[+$field+].value;
        chooser_window = window.open("/management/search[$if ($req->{type}) $]/[+$req->{school}+]/[+$req->{course_id}+][$endif$]?type=[+$type+]&parent-form=[+$pform+]&fill-field=[+$field+]&current_selection=" + current_selection,"[+$type+]chooser", param);
        if (!chooser_window.opener) chooser_window.opener = self;
}
//-->
</script>
        <a class="chooser" href="" onClick="[+$fname+](); return false;">[+$text+]</a>
[$ endsub $]

[$ sub exit_check $]
[-	
	$req = shift;
	if (!$req->{user}){
		exit;
	}
-]
[$ endsub $]
