[$ sub option_item $]
[- $list = shift; -]
<OPTION VALUE="[+$list->[0]+]">[+$list->[1]+]
[$ endsub $]

[$ sub link_item $]
[- $list = shift; -]
[$ if $list->[0] eq '' $]
<TR><TD>[+$list->[1]+]</TD></TR>
[$ else $]
<TR><TD><A HREF="[+$list->[0]+]">[+$list->[1]+]</A></TD></TR>
[$ endif $]
[$ endsub $]

[$ sub first_year $]
<H4 CLASS="title">First Year</H4>
<TABLE CELLPADDING=0 CELLSPACING=3 BORDER=0>
<TR><TD><A HREF="/view/schedule/Dental/670"><B>Schedule</B></A></TD></TR>
[$ if ($affiliation eq 'Dental' && ($year eq 'First')) $]
[$ foreach $listitem @first_year_list $]
[- link_item ($listitem) -]
[$ endforeach $]
[$ else $]
<FORM METHO="post">
<TR><TD><SELECT WIDTH=50 NAME="gotocourse">
<OPTION VALUE="">Select a course
[$ foreach $listitem @first_year_list $]
[- option_item ($listitem) -]
[$ endforeach $]
</SELECT></TD></TR>
<TR><TD><INPUT TYPE="SUBMIT" VALUE="Go to course"></TD></TR>
</FORM>
[$ endif $]
</TABLE>
[$ endsub $]

[$ sub second_year $]
<H4 CLASS="title">Second Year</H4>
<TABLE CELLPADDING=0 CELLSPACING=3 BORDER=0>
<TR><TD><A HREF="/view/schedule/Dental/250"><B>Schedule</B></A></TD></TR>
[$ if ($affiliation eq 'Dental' && $year eq 'Second') $]
[$ foreach $listitem @second_year_list $]
[- link_item ($listitem) -]
[$ endforeach $]
[$ else $]
<FORM METHO="post">
<TR><TD><SELECT WIDTH=50 NAME="gotocourse">
<OPTION VALUE="">Select a course
[$ foreach $listitem @second_year_list $]
[- option_item ($listitem) -]
[$ endforeach $]
</SELECT></TD></TR>
<TR><TD><INPUT TYPE="SUBMIT" VALUE="Go to course"></TD></TR>
</FORM>
[$ endif $]
</TABLE>
[$ endsub $]

[$ sub third_year $]
<H4 CLASS="title">Third Year</H4>
<TABLE CELLPADDING=0 CELLSPACING=3 BORDER=0>
[$ if ($affiliation eq 'Dental' && $year eq 'Third') $]
