[$ if ($affiliation eq 'Dental' && $year eq 'First') $]
[- first_year -]
[- second_year -]
[- third_year -]
[- fourth_year -]

[$ elsif ($affiliation eq 'Dental' && $year eq 'Second') $]
[- second_year -]
[- first_year -]
[- third_year -]
[- fourth_year -]

[$ elsif ($affiliation eq 'Dental' && $year eq 'Third') $]
[- third_year -]
[- first_year -]
[- second_year -]
[- fourth_year -]

[$ elsif ($affiliation eq 'Dental' && $year eq 'Fourth') $]
[- fourth_year -]
[- first_year -]
[- second_year -]
[- third_year -] 
[$ else $]

[- first_year -]
[- second_year -]
[- third_year -]
[- fourth_year -]
[$ endif $]





