#!/usr/bin/python

import polib, sys, re

po_path1 = sys.argv[1]
po1 = polib.pofile(po_path1)
po_path2 = sys.argv[2]
po2 = polib.pofile(po_path2)
catalog = {}

for entry in po1:
	catalog[entry.msgid] = entry.msgstr

for entry in po2
	if (entry.msgid in catalog):
		entry.msgstr = catalog[entry.msgid]

po2.save(po_path2)
