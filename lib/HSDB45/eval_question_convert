#!/usr/bin/perl -w

use strict;
use HSDB45::Eval;
use HSDB4::test::setup;
use XML::Twig;
use Getopt::Long;

my ($school, $eval_id);
GetOptions ("school=s" => \$school,
	    "eval_id=i" => \$eval_id);

my $eval = HSDB45::Eval->new( _school => $school, _id => $eval_id );

for my $question ( $eval->questions() ) {
    $question->body()->elt()->print();
    print "\n\n";
}

1;
__END__
