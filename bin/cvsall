#!/usr/bin/perl
use FindBin;
use lib "$FindBin::Bin/../lib";

my $args = join(' ' => @ARGV);
system("cvs co " . $args . " apache");
chdir('./apache');
system("cvs co " . $args . " HSCML");
system("cvs co " . $args . " XSL");
chdir('./lib/perl');
system("cvs co " . $args . " HSDB4");
system("cvs co " . $args . " HSDB45");
system("cvs co " . $args . " Forum");
system("cvs co " . $args . " XML");
system("cvs co " . $args . " HTML");
system("cvs co " . $args . " HTML");
system("cvs co " . $args . " MySQL");
system("cvs co " . $args . " TestDataInitializer");
system("cvs co " . $args . " Webtest");
